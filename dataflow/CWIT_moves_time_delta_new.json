{
	"name": "CWIT_moves_time_delta_new",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"linkedService": {
						"referenceName": "linkedService2",
						"type": "LinkedServiceReference"
					},
					"name": "ecevents"
				}
			],
			"sinks": [
				{
					"linkedService": {
						"referenceName": "linkedService2",
						"type": "LinkedServiceReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "filter1"
				},
				{
					"name": "alterRow1"
				},
				{
					"name": "filter2"
				},
				{
					"name": "derivedColumn2"
				},
				{
					"name": "aggregate1"
				},
				{
					"name": "select1"
				},
				{
					"name": "filter4"
				}
			],
			"scriptLines": [
				"source(output(",
				"          ecEventGkey as long,",
				"          yardGkey as short,",
				"          YardID as string,",
				"          FcyID as string,",
				"          cheKind as string,",
				"          CHE_Name as string,",
				"          eventType as short,",
				"          eventSubType as short,",
				"          eventTypeDescription as string,",
				"          fromCheIdName as string,",
				"          ToCheIdName as string,",
				"          unit as string,",
				"          pow as string,",
				"          pool as string,",
				"          workQueue as string,",
				"          travelDistance as short,",
				"          moveKind as string,",
				"          isTwin as boolean,",
				"          startDistance as short,",
				"          workAssignment as double,",
				"          unitRef as string,",
				"          tran_ID as string,",
				"          locType as string,",
				"          locID as string,",
				"          locSlot as string,",
				"          opsPosID as string,",
				"          unladenLoctype as string,",
				"          unladenLocID as string,",
				"          unladenLocSlot as string,",
				"          ladenLoctype as string,",
				"          ladenLocID as string,",
				"          laden_LocSlot as string,",
				"          EcEventTime as timestamp,",
				"          cheID as long,",
				"          Year as integer,",
				"          Month as integer,",
				"          Week as integer,",
				"          OPERATOR_ID as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'delta',",
				"     fileSystem: 'cwit',",
				"     folderPath: 'silver/ecevents') ~> ecevents",
				"derivedColumn2 filter(iif(moveKind==\"DSCH\", (cheKind==\"ITV\" || cheKind==\"QC\"), iif(moveKind==\"LOAD\", (cheKind==\"RTG\" || cheKind==\"QC\"),false())) ) ~> filter1",
				"select1 alterRow(upsertIf(true())) ~> alterRow1",
				"ecevents filter(eventTypeDescription=='CMPL'&& (cheKind=='RTG' || cheKind=='ITV' || cheKind=='QC') && not(isNull(EcEventTime))) ~> filter2",
				"filter2 derive(Date = toDate(EcEventTime)) ~> derivedColumn2",
				"filter1 aggregate(groupBy(Date,",
				"          unit,",
				"          eventTypeDescription,",
				"          moveKind,",
				"          Year,",
				"          Month,",
				"          Week),",
				"     is_valid_move = count(ecEventGkey)>=2,",
				"          t_execution_delta = (max(EcEventTime) - min(EcEventTime))/60000,",
				"          execution_gkey = concat(toString(max(ecEventGkey)), \"_\", max(unit), \"_\", toString(min(ecEventGkey))),",
				"          exec_delta_15 = iif(((max(EcEventTime) - min(EcEventTime))/60000) > 15, true(), false()),",
				"     exec_time := (max(EcEventTime) - min(EcEventTime))/60000) ~> aggregate1",
				"filter4 select(mapColumn(",
				"          Date,",
				"          unit,",
				"          eventTypeDescription,",
				"          moveKind,",
				"          Year,",
				"          Month,",
				"          Week,",
				"          is_valid_move,",
				"          t_execution_delta,",
				"          execution_gkey,",
				"          exec_delta_15",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"aggregate1 filter(is_valid_move) ~> filter4",
				"alterRow1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'delta',",
				"     fileSystem: 'cwit',",
				"     folderPath: 'gold/moves_time_delta',",
				"     mergeSchema: false,",
				"     autoCompact: false,",
				"     optimizedWrite: false,",
				"     vacuum: 0,",
				"     deletable: false,",
				"     insertable: false,",
				"     updateable: false,",
				"     upsertable: true,",
				"     keys:['ecEventGkey'],",
				"     umask: 0777,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1"
			]
		}
	}
}