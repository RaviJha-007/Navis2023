{
	"name": "FLD_BHCT",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"linkedService": {
						"referenceName": "linkedService1",
						"type": "LinkedServiceReference"
					},
					"name": "DeckingHistory"
				}
			],
			"sinks": [
				{
					"linkedService": {
						"referenceName": "linkedService2",
						"type": "LinkedServiceReference"
					},
					"name": "fld"
				}
			],
			"transformations": [
				{
					"name": "select2"
				},
				{
					"name": "filter1"
				},
				{
					"name": "derivedColumn1"
				},
				{
					"name": "AlterRow1"
				},
				{
					"name": "window1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          gkey as string,",
				"          uyv_gkey as string,",
				"          unit_id as string,",
				"          time_stamp as timestamp,",
				"          decking_type as string,",
				"          node_id as string,",
				"          yard as string,",
				"          from_pos_loctype as string,",
				"          from_pos_locid as string,",
				"          from_pos_loc_gkey as string,",
				"          from_pos_slot as string,",
				"          from_pos_name as string,",
				"          from_pos_bin as string,",
				"          from_pos_tier as string,",
				"          to_pos_locid as string,",
				"          to_pos_loctype as string,",
				"          to_pos_loc_gkey as string,",
				"          to_pos_slot as string,",
				"          to_pos_name as string,",
				"          to_pos_bin as string,",
				"          to_pos_tier as string,",
				"          total_score as string,",
				"          execution_time as string,",
				"          phase as string,",
				"          Year as integer,",
				"          Month as integer,",
				"          Week as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: true,",
				"     moveFiles: ['/bronze/deckinghistory/load','bronze/deckinghistory/archive'],",
				"     format: 'delta',",
				"     fileSystem: 'bhct',",
				"     folderPath: 'silver/deckings') ~> DeckingHistory",
				"DeckingHistory select(mapColumn(",
				"          gkey,",
				"          uyv_gkey,",
				"          unit_id,",
				"          decking_type,",
				"          node_id,",
				"          yard,",
				"          from_pos_loctype,",
				"          from_pos_locid,",
				"          from_pos_loc_gkey,",
				"          from_pos_slot,",
				"          from_pos_name,",
				"          from_pos_bin,",
				"          from_pos_tier,",
				"          to_pos_loctype,",
				"          to_pos_locid,",
				"          to_pos_loc_gkey,",
				"          to_pos_slot,",
				"          to_pos_name,",
				"          to_pos_bin,",
				"          to_pos_tier,",
				"          total_score,",
				"          execution_time,",
				"          phase,",
				"          time_stamp,",
				"          Year,",
				"          Month,",
				"          Week",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select2",
				"select2 filter((decking_type == 'Bump Intruder' || decking_type == 'Vessel Discharge' || decking_type == 'Vessel Discharge - Twin' || decking_type == 'Gate Receival Assign Position') && to_pos_slot=='FLD') ~> filter1",
				"filter1 derive(Decking_Type_Actual = iif((decking_type == 'Vessel Discharge' || decking_type == 'Vessel Discharge - Twin'), 'Vessel Discharge',decking_type),",
				"          Year_Week = (Year * 100) + Week,",
				"          Year_Month = (Year * 100) + Month,",
				"          Date = toDate(time_stamp)) ~> derivedColumn1",
				"window1 alterRow(upsertIf(true())) ~> AlterRow1",
				"derivedColumn1 window(over(Date,",
				"          unit_id,",
				"          Decking_Type_Actual),",
				"     asc(time_stamp, true),",
				"     FLD_Actual_count = iif((rank() > 1), 0, 1)) ~> window1",
				"AlterRow1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'delta',",
				"     fileSystem: 'bhct',",
				"     folderPath: 'gold/fld',",
				"     mergeSchema: false,",
				"     autoCompact: true,",
				"     optimizedWrite: true,",
				"     vacuum: 0,",
				"     deletable: false,",
				"     insertable: false,",
				"     updateable: false,",
				"     upsertable: true,",
				"     keys:['gkey','uyv_gkey','unit_id','Date'],",
				"     umask: 0777,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     mapColumn(",
				"          gkey,",
				"          uyv_gkey,",
				"          unit_id,",
				"          decking_type,",
				"          from_pos_locid,",
				"          from_pos_slot,",
				"          from_pos_name,",
				"          from_pos_tier,",
				"          to_pos_locid,",
				"          to_pos_slot,",
				"          to_pos_name,",
				"          to_pos_tier,",
				"          total_score,",
				"          execution_time,",
				"          phase,",
				"          time_stamp,",
				"          Year,",
				"          Month,",
				"          Week,",
				"          Decking_Type_Actual,",
				"          Year_Week,",
				"          Year_Month,",
				"          Date,",
				"          fld_actual_count = FLD_Actual_count",
				"     )) ~> fld"
			]
		}
	}
}