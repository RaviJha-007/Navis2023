{
	"name": "MFT_rtg_prod",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"linkedService": {
						"referenceName": "linkedService2",
						"type": "LinkedServiceReference"
					},
					"name": "ecevents"
				},
				{
					"linkedService": {
						"referenceName": "linkedService1",
						"type": "LinkedServiceReference"
					},
					"name": "moves"
				}
			],
			"sinks": [
				{
					"linkedService": {
						"referenceName": "linkedService2",
						"type": "LinkedServiceReference"
					},
					"name": "rtgecevents"
				},
				{
					"linkedService": {
						"referenceName": "linkedService1",
						"type": "LinkedServiceReference"
					},
					"name": "rtgmoves"
				}
			],
			"transformations": [
				{
					"name": "filter1"
				},
				{
					"name": "derivedColumn1"
				},
				{
					"name": "select1"
				},
				{
					"name": "alterRow1"
				},
				{
					"name": "window1"
				},
				{
					"name": "derivedColumn2"
				},
				{
					"name": "filter2"
				},
				{
					"name": "derivedColumn3"
				},
				{
					"name": "aggregate1"
				},
				{
					"name": "aggregate2"
				},
				{
					"name": "filter3"
				},
				{
					"name": "split1"
				},
				{
					"name": "split2"
				},
				{
					"name": "select2"
				},
				{
					"name": "select3"
				},
				{
					"name": "select4"
				},
				{
					"name": "select6Put"
				},
				{
					"name": "select5Fetch"
				},
				{
					"name": "derivedColumn4"
				},
				{
					"name": "derivedColumn5"
				},
				{
					"name": "union1"
				},
				{
					"name": "aggregate3"
				},
				{
					"name": "alterRow2"
				}
			],
			"scriptLines": [
				"parameters{",
				"     Customer as string ('malta')",
				"}",
				"source(output(",
				"          ecEventGkey as long,",
				"          yardGkey as long,",
				"          YardID as string,",
				"          FcyID as string,",
				"          cheKind as string,",
				"          cheName as string,",
				"          cheID as long,",
				"          type as short,",
				"          subType as short,",
				"          typeDescription as string,",
				"          fromCheIdName as short,",
				"          ToCheIdName as short,",
				"          unitIdName as string,",
				"          powName as string,",
				"          poolName as string,",
				"          workQueue as string,",
				"          travelDistance as short,",
				"          moveKind as string,",
				"          isTwin as boolean,",
				"          startDistance as short,",
				"          unitReference as string,",
				"          tranId as string,",
				"          locType as string,",
				"          locId as string,",
				"          locSlot as string,",
				"          opsPosId as string,",
				"          unladenLoctype as string,",
				"          unladenLocId as string,",
				"          unladenLocSlot as string,",
				"          ladenLoctype as string,",
				"          ladenLocId as string,",
				"          laden_LocSlot as string,",
				"          operatorName as string,",
				"          EcEventTime as timestamp,",
				"          Year as integer,",
				"          Month as integer,",
				"          Week as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: true,",
				"     format: 'delta',",
				"     fileSystem: ($Customer),",
				"     folderPath: 'silver/ecevents') ~> ecevents",
				"source(output(",
				"          mve_gkey as string,",
				"          Unit_Nbr as string,",
				"          Facility_Id as string,",
				"          Complex_Id as string,",
				"          Yard_Id as string,",
				"          Carrier_Visit_Key as string,",
				"          Event_Type_Id as string,",
				"          Move_Kind as string,",
				"          From_Location as string,",
				"          To_Location as string,",
				"          From_Position_Name as string,",
				"          To_Position_Name as string,",
				"          Carry_CHE_Operator_Id as string,",
				"          Fetch_CHE_Operator_Id as string,",
				"          Put_CHE_Operator_Id as string,",
				"          Fetch_CHE_Id as long,",
				"          Fetch_CHE_Kind as string,",
				"          Carry_CHE_Id as long,",
				"          Carry_CHE_Kind as string,",
				"          Put_CHE_Id as long,",
				"          Put_CHE_Kind as string,",
				"          QC_CHE_Id as long,",
				"          QC_CHE_Kind as string,",
				"          t_carry_complete as timestamp,",
				"          t_put as timestamp,",
				"          t_fetch as timestamp,",
				"          t_dispatch as timestamp,",
				"          t_discharge as timestamp,",
				"          t_carry_dispatch as timestamp,",
				"          t_carry_fetch_ready as timestamp,",
				"          t_carry_put_ready as timestamp,",
				"          dist_carry as string,",
				"          dist_start as string,",
				"          Twin_Fetch as string,",
				"          Twin_Carry as string,",
				"          Twin_Put as string,",
				"          Category_Id as string,",
				"          Freight_Kind_Id as string,",
				"          to_pos_gkey as string,",
				"          fm_pos_gkey as string,",
				"          Restow_Reason as string,",
				"          Container_Line_Id as string,",
				"          Container_Line_Role as string,",
				"          pow as string,",
				"          ufv_gkey as string,",
				"          Berth_Id as string,",
				"          Goods_And_Ctr_Wt_Kg as string,",
				"          active_ufv as string,",
				"          Requires_Power_Flag as string,",
				"          OOG_Flag as string,",
				"          Shipper_Id as string,",
				"          Consignee_Id as string,",
				"          Commodity_Id as string,",
				"          ATD as timestamp,",
				"          Rehandles as string,",
				"          ATA as timestamp,",
				"          Year as integer,",
				"          Month as integer,",
				"          Week as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: true,",
				"     format: 'delta',",
				"     fileSystem: ($Customer),",
				"     folderPath: 'silver/moves') ~> moves",
				"aggregate1 filter(not(isNull(ecEventGkey)) && cheKind == 'RTG' && (typeDescription == 'LGON' || typeDescription == 'LGOFF')) ~> filter1",
				"filter1 derive(Date = :CurrentDate,",
				"          ShiftStartTime = iif((EcEventTime >= :NightShift.Start) && (EcEventTime <= :NightShift.End), :NightShift.Start, iif((EcEventTime >= :MorningShift.Start) && (EcEventTime <= :MorningShift.End), :MorningShift.Start, iif((EcEventTime >= :EveningShift.Start) && (EcEventTime <= :EveningShift.End), :EveningShift.Start, EcEventTime)) ),",
				"          ShiftEndTime = iif((EcEventTime >= :NightShift.Start) && (EcEventTime <= :NightShift.End), :NightShift.End, iif((EcEventTime >= :MorningShift.Start) && (EcEventTime <= :MorningShift.End), :MorningShift.End, iif((EcEventTime >= :EveningShift.Start) && (EcEventTime <= :EveningShift.End), :EveningShift.End, EcEventTime)) ),",
				"          ShiftName = iif((EcEventTime >= :NightShift.Start) && (EcEventTime <= :NightShift.End), :NightShift.Name, iif((EcEventTime >= :MorningShift.Start) && (EcEventTime <= :MorningShift.End), :MorningShift.Name, iif((EcEventTime >= :EveningShift.Start) && (EcEventTime <= :EveningShift.End), :EveningShift.Name, \"\"))),",
				"          ShiftDateName = toString(:CurrentDate)+ \" \" + iif((EcEventTime >= :NightShift.Start) && (EcEventTime <= :NightShift.End), :NightShift.Name, iif((EcEventTime >= :MorningShift.Start) && (EcEventTime <= :MorningShift.End), :MorningShift.Name, iif((EcEventTime >= :EveningShift.Start) && (EcEventTime <= :EveningShift.End), :EveningShift.Name, \"\"))),",
				"          customer = $Customer,",
				"          CurrentDate := toDate(EcEventTime),",
				"          NightShift := @(Name=\"Night\",",
				"          Start=toTimestamp((toString(toDate(EcEventTime)) + \" \" + \"00:00:00\"), \"yyyy-MM-dd HH:mm:ss\"),",
				"          End=toTimestamp((toString(toDate(EcEventTime)) + \" \" + \"07:59:59\"), \"yyyy-MM-dd HH:mm:ss\")),",
				"          MorningShift := @(Name=\"Morning\",",
				"          Start=toTimestamp((toString(toDate(EcEventTime)) + \" \" + \"08:00:00\"), \"yyyy-MM-dd HH:mm:ss\"),",
				"          End=toTimestamp((toString(toDate(EcEventTime)) + \" \" + \"15:59:59\"), \"yyyy-MM-dd HH:mm:ss\")),",
				"          EveningShift := @(Name=\"Evening\",",
				"          Start=toTimestamp((toString(toDate(EcEventTime)) + \" \" + \"16:00:00\"), \"yyyy-MM-dd HH:mm:ss\"),",
				"          End=toTimestamp((toString(toDate(EcEventTime)) + \" \" + \"23:59:59\"), \"yyyy-MM-dd HH:mm:ss\"))) ~> derivedColumn1",
				"derivedColumn1 select(mapColumn(",
				"          ecEventGkey,",
				"          yardGkey,",
				"          YardID,",
				"          FcyID,",
				"          cheKind,",
				"          cheName,",
				"          cheID,",
				"          type,",
				"          subType,",
				"          typeDescription,",
				"          fromCheIdName,",
				"          ToCheIdName,",
				"          unitIdName,",
				"          powName,",
				"          poolName,",
				"          workQueue,",
				"          travelDistance,",
				"          moveKind,",
				"          isTwin,",
				"          startDistance,",
				"          locSlot,",
				"          operatorName,",
				"          customer,",
				"          EcEventTime,",
				"          Year,",
				"          Month,",
				"          Week,",
				"          Date,",
				"          ShiftStartTime,",
				"          ShiftEndTime,",
				"          ShiftName,",
				"          ShiftDateName",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"derivedColumn3 alterRow(upsertIf(true())) ~> alterRow1",
				"select1 window(over(cheID,",
				"          ShiftDateName),",
				"     asc(EcEventTime, true),",
				"     asc(ecEventGkey, true),",
				"     typeDescription_next = lead(typeDescription),",
				"          EcEventTime_next = lead(EcEventTime),",
				"          typeDescription_prev = lag(typeDescription)) ~> window1",
				"window1 derive(ActualEvent = iif(typeDescription==\"LGOFF\" && isNull(typeDescription_prev), \"LGON\", typeDescription),",
				"          ActualEvent_next = iif((typeDescription==\"LGOFF\" && isNull(typeDescription_prev)) || (typeDescription==\"LGON\" && isNull(typeDescription_next)), \"LGOFF\", typeDescription_next),",
				"          ActualTime = iif(typeDescription==\"LGOFF\" && isNull(typeDescription_prev), ShiftStartTime, EcEventTime),",
				"          ActualTime_next = iif(typeDescription==\"LGOFF\" && isNull(typeDescription_prev), EcEventTime, iif(typeDescription==\"LGON\" && isNull(typeDescription_next), ShiftEndTime,EcEventTime_next))) ~> derivedColumn2",
				"derivedColumn2 filter(ActualEvent==\"LGON\" && ActualEvent_next==\"LGOFF\") ~> filter2",
				"filter2 derive(WorktimeSeconds = (ActualTime_next - ActualTime)/1000) ~> derivedColumn3",
				"ecevents aggregate(groupBy(ecEventGkey),",
				"     each(match(name!='ecEventGkey'), $$ = first($$))) ~> aggregate1",
				"moves aggregate(groupBy(mve_gkey),",
				"     each(match(name!='mve_gkey'), $$ = first($$))) ~> aggregate2",
				"aggregate2 filter(not(isNull(mve_gkey))) ~> filter3",
				"filter3 split(in([\"DLVR\",\"LOAD\",\"SHIFT\"], Move_Kind) && Fetch_CHE_Kind==\"RTG\" && not(isNull(Fetch_CHE_Id)),",
				"     in([\"DSCH\",\"RECV\"], Move_Kind) && Put_CHE_Kind==\"RTG\"  && not(isNull(Put_CHE_Id)),",
				"     Move_Kind==\"YARD\",",
				"     disjoint: true) ~> split1@(LSD, RD, Y)",
				"split1@Y split(Fetch_CHE_Kind==\"RTG\" && Put_CHE_Kind==\"RTG\" && Fetch_CHE_Id==Put_CHE_Id && not(isNull(Put_CHE_Id)),",
				"     Fetch_CHE_Kind==\"RTG\" && Put_CHE_Kind==\"RTG\" && Fetch_CHE_Id!=Put_CHE_Id && not(isNull(Fetch_CHE_Id)) && not(isNull(Put_CHE_Id)),",
				"     disjoint: true) ~> split2@(YE, YNE)",
				"split1@LSD select(mapColumn(",
				"          Move_Primary_key = mve_gkey,",
				"          Unit_Nbr,",
				"          Facility_Id,",
				"          Complex_Id,",
				"          Yard_Id,",
				"          Carrier_Visit_Key,",
				"          Event_Type_Id,",
				"          Move_Kind,",
				"          From_Location,",
				"          To_Location,",
				"          From_Position_Name,",
				"          To_Position_Name,",
				"          CHE_Operator_Id = Fetch_CHE_Operator_Id,",
				"          CHE_Id = Fetch_CHE_Id,",
				"          CHE_Kind = Fetch_CHE_Kind,",
				"          move_time = t_fetch,",
				"          dist_carry,",
				"          dist_start,",
				"          Category_Id,",
				"          Freight_Kind_Id,",
				"          pow,",
				"          ufv_gkey,",
				"          Year,",
				"          Month,",
				"          Week",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select2",
				"split1@RD select(mapColumn(",
				"          Move_Primary_key = mve_gkey,",
				"          Unit_Nbr,",
				"          Facility_Id,",
				"          Complex_Id,",
				"          Yard_Id,",
				"          Carrier_Visit_Key,",
				"          Event_Type_Id,",
				"          Move_Kind,",
				"          From_Location,",
				"          To_Location,",
				"          From_Position_Name,",
				"          To_Position_Name,",
				"          CHE_Operator_Id = Put_CHE_Operator_Id,",
				"          CHE_Id = Put_CHE_Id,",
				"          CHE_Kind = Put_CHE_Kind,",
				"          move_time = t_put,",
				"          dist_carry,",
				"          dist_start,",
				"          Category_Id,",
				"          Freight_Kind_Id,",
				"          pow,",
				"          ufv_gkey,",
				"          Year,",
				"          Month,",
				"          Week",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select3",
				"split2@YE select(mapColumn(",
				"          Move_Primary_key = mve_gkey,",
				"          Unit_Nbr,",
				"          Facility_Id,",
				"          Complex_Id,",
				"          Yard_Id,",
				"          Carrier_Visit_Key,",
				"          Event_Type_Id,",
				"          Move_Kind,",
				"          From_Location,",
				"          To_Location,",
				"          From_Position_Name,",
				"          To_Position_Name,",
				"          CHE_Operator_Id = Put_CHE_Operator_Id,",
				"          CHE_Id = Put_CHE_Id,",
				"          CHE_Kind = Put_CHE_Kind,",
				"          move_time = t_put,",
				"          dist_carry,",
				"          dist_start,",
				"          Category_Id,",
				"          Freight_Kind_Id,",
				"          pow,",
				"          ufv_gkey,",
				"          Year,",
				"          Month,",
				"          Week",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select4",
				"split2@YNE select(mapColumn(",
				"          Move_Primary_key = mve_gkey,",
				"          Unit_Nbr,",
				"          Facility_Id,",
				"          Complex_Id,",
				"          Yard_Id,",
				"          Carrier_Visit_Key,",
				"          Event_Type_Id,",
				"          Move_Kind,",
				"          From_Location,",
				"          To_Location,",
				"          From_Position_Name,",
				"          To_Position_Name,",
				"          CHE_Operator_Id = Put_CHE_Operator_Id,",
				"          CHE_Id = Put_CHE_Id,",
				"          CHE_Kind = Put_CHE_Kind,",
				"          move_time = t_put,",
				"          dist_carry,",
				"          dist_start,",
				"          Category_Id,",
				"          Freight_Kind_Id,",
				"          pow,",
				"          ufv_gkey,",
				"          Year,",
				"          Month,",
				"          Week",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select6Put",
				"split2@YNE select(mapColumn(",
				"          Move_Primary_key = mve_gkey,",
				"          Unit_Nbr,",
				"          Facility_Id,",
				"          Complex_Id,",
				"          Yard_Id,",
				"          Carrier_Visit_Key,",
				"          Event_Type_Id,",
				"          Move_Kind,",
				"          From_Location,",
				"          To_Location,",
				"          From_Position_Name,",
				"          To_Position_Name,",
				"          CHE_Operator_Id = Fetch_CHE_Operator_Id,",
				"          CHE_Id = Fetch_CHE_Id,",
				"          CHE_Kind = Fetch_CHE_Kind,",
				"          move_time = t_fetch,",
				"          dist_carry,",
				"          dist_start,",
				"          Category_Id,",
				"          Freight_Kind_Id,",
				"          pow,",
				"          ufv_gkey,",
				"          Year,",
				"          Month,",
				"          Week",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select5Fetch",
				"select5Fetch derive(Move_Primary_key = toString(Move_Primary_key) + \"-\" + toString(CHE_Id)) ~> derivedColumn4",
				"select6Put derive(Move_Primary_key = toString(Move_Primary_key) + \"-\" + toString(CHE_Id)) ~> derivedColumn5",
				"select3, select4, select2, derivedColumn4, derivedColumn5 union(byName: true)~> union1",
				"union1 aggregate(groupBy(Move_Primary_key),",
				"     each(match(name!='Move_Primary_key'), $$ = first($$))) ~> aggregate3",
				"aggregate3 alterRow(upsertIf(true())) ~> alterRow2",
				"alterRow1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'delta',",
				"     fileSystem: ($Customer),",
				"     folderPath: 'gold/rtg_ecevents',",
				"     mergeSchema: false,",
				"     autoCompact: false,",
				"     optimizedWrite: false,",
				"     vacuum: 0,",
				"     deletable: false,",
				"     insertable: false,",
				"     updateable: false,",
				"     upsertable: true,",
				"     keys:['ecEventGkey'],",
				"     umask: 0777,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> rtgecevents",
				"alterRow2 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'delta',",
				"     fileSystem: ($Customer),",
				"     folderPath: 'gold/rtg_moves',",
				"     mergeSchema: false,",
				"     autoCompact: false,",
				"     optimizedWrite: false,",
				"     vacuum: 0,",
				"     deletable: false,",
				"     insertable: false,",
				"     updateable: false,",
				"     upsertable: true,",
				"     keys:['Move_Primary_key'],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> rtgmoves"
			]
		}
	}
}