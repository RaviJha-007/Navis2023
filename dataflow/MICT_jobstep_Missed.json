{
	"name": "MICT_jobstep_Missed",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"linkedService": {
						"referenceName": "linkedService1",
						"type": "LinkedServiceReference"
					},
					"name": "source1"
				},
				{
					"linkedService": {
						"referenceName": "linkedService1",
						"type": "LinkedServiceReference"
					},
					"name": "xpseceuser"
				}
			],
			"sinks": [
				{
					"linkedService": {
						"referenceName": "linkedService1",
						"type": "LinkedServiceReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "filter1"
				},
				{
					"name": "window1"
				},
				{
					"name": "derivedColumn1"
				},
				{
					"name": "AlterRow1"
				},
				{
					"name": "join1"
				},
				{
					"name": "select1"
				},
				{
					"name": "aggregate1"
				},
				{
					"name": "derivedColumn2"
				},
				{
					"name": "filter2"
				},
				{
					"name": "select2"
				}
			],
			"scriptLines": [
				"source(output(",
				"          ecEventGkey as long,",
				"          yardGkey as string,",
				"          YardID as string,",
				"          FcyID as string,",
				"          cheKind as string,",
				"          CHE_Name as string,",
				"          eventType as short,",
				"          eventSubType as short,",
				"          eventTypeDescription as string,",
				"          fromCheIdName as string,",
				"          ToCheIdName as string,",
				"          unit as string,",
				"          pow as string,",
				"          pool as string,",
				"          workQueue as string,",
				"          travelDistance as short,",
				"          moveKind as string,",
				"          isTwin as string,",
				"          startDistance as short,",
				"          workAssignment as float,",
				"          unitRef as string,",
				"          tran_ID as string,",
				"          locType as string,",
				"          locID as string,",
				"          locSlot as string,",
				"          opsPosID as string,",
				"          unladenLoctype as string,",
				"          unladenLocID as string,",
				"          unladenLocSlot as string,",
				"          ladenLoctype as string,",
				"          ladenLocID as string,",
				"          laden_LocSlot as string,",
				"          lastEstMoveTime as string,",
				"          EcEventTime as timestamp,",
				"          cheID as long,",
				"          Year as integer,",
				"          Month as integer,",
				"          Week as integer,",
				"          operator as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'delta',",
				"     fileSystem: 'mict',",
				"     folderPath: 'silver/ecevents_v2') ~> source1",
				"source(output(",
				"          gkey as integer,",
				"          user_id as string,",
				"          name as string,",
				"          yard as string",
				"     ),",
				"     useSchema: false,",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'delimited',",
				"     fileSystem: 'mict',",
				"     folderPath: 'bronze/xpsecuser',",
				"     columnDelimiter: ',',",
				"     escapeChar: '\\\\',",
				"     quoteChar: '\\\"',",
				"     columnNamesAsHeader: true) ~> xpseceuser",
				"select2 filter(not(isNull(ecEventGkey)) && cheKind == \"ITV\" && (moveKind == \"DSCH\"|| moveKind == \"LOAD\")) ~> filter1",
				"filter1 window(over(cheID),",
				"     asc(EcEventTime, true),",
				"     asc(ecEventGkey, true),",
				"     eventTypeDescription_nxt = lead(eventTypeDescription),",
				"          eventTypeDescription_lag = lag(eventTypeDescription)) ~> window1",
				"window1 derive(jobStep = iif(eventTypeDescription == 'TVDR',\r",
				"    iif(eventTypeDescription_nxt == 'AVDR' && eventTypeDescription_lag == 'AYCO', 1, 0),\r",
				"    iif(eventTypeDescription == 'TYDR',\r",
				"        iif(eventTypeDescription_nxt == 'AYDR' && eventTypeDescription_lag == 'AVCO', 1, 0),\r",
				"        toInteger(null())\r",
				"    )\r",
				"),",
				"          Types = case(\r",
				"    eventTypeDescription == 'TVDR', \r",
				"        iif((eventTypeDescription_nxt != 'AVDR' || isNull(eventTypeDescription_nxt)) && (eventTypeDescription_lag != 'AYCO' || isNull(eventTypeDescription_lag)), 'Miss at Vessel and Row',\r",
				"        iif(eventTypeDescription_nxt != 'AVDR' || isNull(eventTypeDescription_nxt), 'Load-Miss Arrive at Vessel',\r",
				"        iif(eventTypeDescription_lag != 'AYCO' || isNull(eventTypeDescription_lag), 'Load-Miss Arrive at Row', 'No Violation'))),\r",
				"    \r",
				"    eventTypeDescription == 'TYDR', \r",
				"        iif((eventTypeDescription_nxt != 'AYDR' || isNull(eventTypeDescription_nxt)) && (eventTypeDescription_lag != 'AVCO' || isNull(eventTypeDescription_lag)), 'Miss at Vessel and Row',\r",
				"        iif(eventTypeDescription_nxt != 'AYDR' || isNull(eventTypeDescription_nxt), 'DSCH-Miss Arrive at Row',\r",
				"        iif(eventTypeDescription_lag != 'AVCO' || isNull(eventTypeDescription_lag), 'DSCH-Miss Arrive at Vessel', 'No Violation'))),\r",
				"    \r",
				"    toString(null())\r",
				"),",
				"          operatorName = regexReplace(operatorName, `[^a-zA-Z0-9]`, '')) ~> derivedColumn1",
				"select1 alterRow(upsertIf(true())) ~> AlterRow1",
				"derivedColumn1, filter2 join(operatorName == USER_ID,",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join1",
				"join1 select(mapColumn(",
				"          ecEventGkey,",
				"          yardGkey,",
				"          YardID,",
				"          FcyID,",
				"          cheKind,",
				"          CHE_Name = cheName,",
				"          eventType = type,",
				"          eventSubType = subType,",
				"          eventTypeDescription,",
				"          unit,",
				"          pow = powName,",
				"          pool = poolName,",
				"          travelDistance,",
				"          moveKind,",
				"          startDistance,",
				"          EcEventTime,",
				"          cheID,",
				"          Year,",
				"          Month,",
				"          Week,",
				"          OperatorID = operatorName,",
				"          eventTypeDescription_lag,",
				"          eventTypeDescription_nxt,",
				"          OperatorName = NAME,",
				"          jobStep,",
				"          Types",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"derivedColumn2 aggregate(groupBy(derivedColumn2@USER_ID),",
				"     NAME = last(derivedColumn2@NAME)) ~> aggregate1",
				"xpseceuser derive(USER_ID = regexReplace(user_id, `[^a-zA-Z0-9]`, ''),",
				"          NAME = regexReplace(name, `[^a-zA-Z0-9]`, ' ')) ~> derivedColumn2",
				"aggregate1 filter(not(isNull(USER_ID))) ~> filter2",
				"source1 select(mapColumn(",
				"          ecEventGkey,",
				"          yardGkey,",
				"          YardID,",
				"          FcyID,",
				"          cheKind,",
				"          cheName = CHE_Name,",
				"          cheID,",
				"          type = eventType,",
				"          subType = eventSubType,",
				"          eventTypeDescription,",
				"          fromCheIdName,",
				"          ToCheIdName,",
				"          unit,",
				"          powName = pow,",
				"          poolName = pool,",
				"          workQueue,",
				"          travelDistance,",
				"          moveKind,",
				"          isTwin,",
				"          startDistance,",
				"          unitReference = unitRef,",
				"          tranId = tran_ID,",
				"          locType,",
				"          locId = locID,",
				"          locSlot,",
				"          opsPosId = opsPosID,",
				"          unladenLoctype,",
				"          unladenLocId = unladenLocID,",
				"          unladenLocSlot,",
				"          ladenLoctype,",
				"          ladenLocId = ladenLocID,",
				"          laden_LocSlot,",
				"          operatorName = operator,",
				"          EcEventTime,",
				"          Year,",
				"          Month,",
				"          Week",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select2",
				"AlterRow1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'delta',",
				"     fileSystem: 'mict',",
				"     folderPath: 'gold/jobstep_missed',",
				"     mergeSchema: true,",
				"     autoCompact: true,",
				"     optimizedWrite: true,",
				"     vacuum: 0,",
				"     deletable: false,",
				"     insertable: false,",
				"     updateable: false,",
				"     upsertable: true,",
				"     keys:['ecEventGkey'],",
				"     umask: 0777,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1"
			]
		}
	}
}