				"     folderPath: 'gold/sc_laden_unladen',",
				"     mergeSchema: false,",
				"     autoCompact: false,",
				"     optimizedWrite: false,",
				"     vacuum: 0,",
				"     deletable: false,",
				"     insertable: false,",
				"     updateable: false,",
				"     upsertable: true,",
				"     keys:['unitIdName','moveKind','Timestamp'],",
				"     umask: 0022,",
				"     preCommands: [],",
