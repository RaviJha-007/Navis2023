{
	"name": "stack_confirmity_MCT_v2",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"linkedService": {
						"referenceName": "linkedService2",
						"type": "LinkedServiceReference"
					},
					"name": "StackConfirmity"
				}
			],
			"sinks": [
				{
					"linkedService": {
						"referenceName": "linkedService2",
						"type": "LinkedServiceReference"
					},
					"name": "StackConformity"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "aggregate1"
				},
				{
					"name": "join1"
				},
				{
					"name": "sort1"
				},
				{
					"name": "filter1"
				},
				{
					"name": "alterRow1"
				},
				{
					"name": "select2"
				},
				{
					"name": "select3"
				},
				{
					"name": "derivedColumn2"
				},
				{
					"name": "filter2"
				},
				{
					"name": "derivedColumn3"
				},
				{
					"name": "aggregate3"
				},
				{
					"name": "sort2"
				},
				{
					"name": "join2"
				},
				{
					"name": "select4"
				},
				{
					"name": "derivedColumn4"
				},
				{
					"name": "derivedColumn5"
				}
			],
			"scriptLines": [
				"source(output(",
				"          ContainerNo as string,",
				"          StackingFactor as string,",
				"          AssignedCHE as string,",
				"          BlockType as string,",
				"          {Eq Type} as string,",
				"          {Equipment Class} as string,",
				"          CurrentPosition as string,",
				"          YardRow as string,",
				"          Cat as string,",
				"          Sts as string,",
				"          SectionFactor as string,",
				"          {Outbound Carrier} as string,",
				"          {Current POD} as string,",
				"          {Gross Weight} as integer,",
				"          {Special Stow1} as string,",
				"          {Special Stow2} as string,",
				"          {Special Stow3} as string,",
				"          {Group Code} as string,",
				"          Line as string",
				"     ),",
				"     useSchema: false,",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     inferDriftedColumnTypes: true,",
				"     ignoreNoFilesFound: true,",
				"     moveFiles: ['/bronze/stackconformitycsvs/load','bronze/stackconformitycsvs/archive'],",
				"     rowUrlColumn: 'FileDate',",
				"     format: 'delimited',",
				"     fileSystem: 'mct',",
				"     folderPath: 'bronze/stackconformitycsvs/load',",
				"     columnDelimiter: ',',",
				"     escapeChar: '\\\\',",
				"     quoteChar: '',",
				"     columnNamesAsHeader: true) ~> StackConfirmity",
				"select3 derive(Pos_WO_Tier = iif(instr(CurrentPosition, \".\")!=0, split(CurrentPosition, \".\")[1], \r",
				"iif(isInteger(right(CurrentPosition, 1)), dropRight(CurrentPosition, 1), \"\")),",
				"          Date = :created_date,",
				"          Week = iif(dayOfWeek(:created_date) == 1,  weekOfYear(:created_date) + 1, weekOfYear(:created_date)),",
				"          Tier = iif(instr(CurrentPosition, \".\")!=0, split(CurrentPosition, \".\")[2], \r",
				"iif( isInteger(right(CurrentPosition, 1)), right(CurrentPosition, 1), \"\")),",
				"          stacking_array := split(StackingFactor, '/'),",
				"          created_date := toDate(split(split(FileDate, \"_\")[2],\".\")[1], 'ddMMyyyy')) ~> derivedColumn1",
				"derivedColumn1 aggregate(groupBy(Date,",
				"          Pos_WO_Tier),",
				"     Stack_Distinct = countDistinct(StackingFactor),",
				"          minTier = min(Tier),",
				"          maxWeight = max({Gross Weight}),",
				"          minWeight = min({Gross Weight})) ~> aggregate1",
				"derivedColumn1, derivedColumn2 join(derivedColumn1@Date == aggregate1@Date",
				"     && derivedColumn1@Pos_WO_Tier == aggregate1@Pos_WO_Tier,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join1",
				"derivedColumn4 sort(asc(Date, true),",
				"     asc(ContainerNo, true),",
				"     asc(Tier, true)) ~> sort1",
				"join1 filter(not(isDate(ContainerNo)) && not(isNull(ContainerNo)) && ContainerNo != '') ~> filter1",
				"select2 alterRow(insertIf(true())) ~> alterRow1",
				"derivedColumn5 select(mapColumn(",
				"          each(match(true()),",
				"               regexReplace($$,' ','_') = $$)",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select2",
				"StackConfirmity select(mapColumn(",
				"          ContainerNo,",
				"          StackingFactor,",
				"          {Eq Type},",
				"          {Equipment Class},",
				"          CurrentPosition,",
				"          YardRow,",
				"          Cat,",
				"          Sts,",
				"          SectionFactor,",
				"          {Outbound Carrier},",
				"          {Current POD},",
				"          {Gross Weight},",
				"          {Group Code},",
				"          Line,",
				"          FileDate",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select3",
				"aggregate1 derive(Stack_Conformity = iif(Stack_Distinct>1, \"Mix\", \"Pure\")) ~> derivedColumn2",
				"filter1 filter(Tier==minTier && not(isNull(minTier)) && not(isNull(Tier))) ~> filter2",
				"aggregate3 derive(Weight_Conformity = iif((maxWeight>=:maxLimit || minWeight<=:minLimit), \"Mix\", \"Pure\"),",
				"          MaxWeightLimit = :maxLimit,",
				"          MinWeightLimit = :minLimit,",
				"          maxLimit := 1.15*refWeight,",
				"          minLimit := refWeight*0.85) ~> derivedColumn3",
				"sort2 aggregate(groupBy(derivedColumn1@Date,",
				"          derivedColumn1@Pos_WO_Tier,",
				"          minTier,",
				"          maxWeight,",
				"          minWeight),",
				"     refWeight = first({Gross Weight}, true())) ~> aggregate3",
				"filter2 sort(asc(aggregate1@Date, true),",
				"     asc(aggregate1@Pos_WO_Tier, true),",
				"     asc(Tier, true),",
				"     partitionLevel: true) ~> sort2",
				"filter1, derivedColumn3 join(aggregate1@Date == aggregate3@Date",
				"     && aggregate1@Pos_WO_Tier == aggregate3@Pos_WO_Tier",
				"     && aggregate1@minTier == aggregate3@minTier",
				"     && aggregate1@maxWeight == aggregate3@maxWeight",
				"     && aggregate1@minWeight == aggregate3@minWeight,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join2",
				"join2 select(mapColumn(",
				"          ContainerNo,",
				"          StackingFactor,",
				"          {Eq Type},",
				"          {Equipment Class},",
				"          CurrentPosition,",
				"          YardRow,",
				"          Cat,",
				"          Sts,",
				"          SectionFactor,",
				"          {Outbound Carrier},",
				"          {Current POD},",
				"          {Gross Weight},",
				"          {Group Code},",
				"          Line,",
				"          FileDate,",
				"          Week,",
				"          Tier,",
				"          Stack_Distinct,",
				"          Stack_Conformity,",
				"          Date = aggregate3@Date,",
				"          Pos_WO_Tier = aggregate3@Pos_WO_Tier,",
				"          minTier = aggregate3@minTier,",
				"          maxWeight = aggregate3@maxWeight,",
				"          minWeight = aggregate3@minWeight,",
				"          refWeight,",
				"          Weight_Conformity,",
				"          MaxWeightLimit,",
				"          MinWeightLimit",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select4",
				"select4 derive(Weight_Conformity = iif(isNull(minTier) || Tier==\"\" , \"N/A\", Weight_Conformity),",
				"          Stack_Conformity = iif(isNull(minTier) || Tier==\"\" , \"N/A\", Stack_Conformity)) ~> derivedColumn4",
				"sort1 derive(Conformity = iif((Weight_Conformity==\"Pure\" && Stack_Conformity==\"Pure\"), \"Pure\", \r",
				"iif((Weight_Conformity==\"Mix\" && Stack_Conformity==\"Pure\"), \"Pure Stack, Mixed Weight\",\r",
				"iif((Weight_Conformity==\"Pure\" && Stack_Conformity==\"Mix\"), \"Mixed Stack, Pure Weight\", \r",
				"iif((Weight_Conformity==\"Mix\" && Stack_Conformity==\"Mix\"), \"Mixed Stack, Mixed Weight\", \"N/A\"))\r",
				")\r",
				")) ~> derivedColumn5",
				"alterRow1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'delta',",
				"     fileSystem: 'mct',",
				"     folderPath: 'gold/stack_confirmity_v2',",
				"     mergeSchema: true,",
				"     autoCompact: false,",
				"     optimizedWrite: false,",
				"     vacuum: 0,",
				"     deletable: false,",
				"     insertable: true,",
				"     updateable: false,",
				"     upsertable: false,",
				"     umask: 0777,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> StackConformity"
			]
		}
	}
}